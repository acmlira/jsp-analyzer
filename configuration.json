{
    "default_metrics": {
        "active": true
    },
    "regex_metrics": [
        {
            "label": "Number of included JSP",
            "regex": "<%@\\s?include",
            "files": false
        },
        {
            "label": "Number of parameterized JSP",
            "regex": "<jsp:include",
            "files": false
        },
        {
            "label": "Probaly autogenerated JSP",
            "regex": "\".jsp\"",
            "files": false
        },
        {
            "label": "Generic JSP reference",
            "regex": "(\"[a-zA-Z0-9/]+.jsp\")|(\".jsp\")",
            "files": true
        }
    ]
}